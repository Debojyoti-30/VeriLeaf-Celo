# Copy this file to .env and set your Sentinel Hub credentials
SENTINEL_CLIENT_ID=your_client_id_here
SENTINEL_CLIENT_SECRET=your_client_secret_here
# Optional: port for the local sentinel server
PORT=4000

# Frontend URLs for local dev
VITE_API_URL=http://localhost:4000
VITE_AI_API_URL=http://localhost:5000

# Contracts
# Deployed VeriLeafClaims contract address (Celo or Alfajores)
VITE_CLAIMS_CONTRACT_ADDRESS=0xYourContractAddressHere
# Optional: if you split responsibilities across contracts
VITE_VERIFIER_CONTRACT_ADDRESS=
